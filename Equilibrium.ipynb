   "source": [
    "from sympy import *"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "x, y = symbols('x y')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 82,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "b, d, r, p = symbols('b d r p')\n",
    "f = d*x + r*x*y\n",
    "g = b*y - p*x*y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[(0, 0), (b/(2*p), -d/(2*r))]"
      ]
     },
     "execution_count": 83,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "dfx = diff(f,x)\n",
    "dfy = diff(f,y)\n",
    "dgx = diff(g,x)\n",
    "dgy = diff(g,y)\n",
    "\n",
    "solve((x*dfx + y*dfy, x*dgx + y*dgy), x,y)"
   ]
  }
 ],
